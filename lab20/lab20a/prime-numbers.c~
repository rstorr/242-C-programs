#include <stdio.h>
#include <stdlib.h>

/*1 if prime 0 if not.*/
int is_prime(int candidate) {
    int i;
    for (i = 2; i < candidate; i++){
        if(candidate % i == 0){ return 0; }
    }
    return 1;
}


int main(void) {
    int candidate = 2;
    int num_printed = 0;

    while (num_printed < 200) {
        if(is_prime(candidate) == 1){
            printf("     %d", candidate);
            num_printed++;
            if (num_printed % 10 == 0){
                printf("\n");
            }
        }
        candidate++;
    }

    return EXIT_SUCCESS;
}
